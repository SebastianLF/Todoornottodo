n.list-item {\n  display: flex;\n  border-bottom: 1px solid #e3e4f1;\n  height: 3.25rem;\n  align-items: stretch;\n  &:last-child {\n    border-bottom: none;\n  }\n}\n\n.list-item__text {\n  font-size: 0.75rem;\n  color: #494c6b;\n}\n\n.list-item__completable {\n  display: flex;\n  flex: 1;\n  cursor: pointer;\n  align-items: center;\n  &--completed {\n    text-decoration: line-through;\n  }\n}\n\n.list-item__select-box {\n  padding: 0 0.6875rem 0 1.25rem;\n}\n\n.list-item__select-wrapper {\n  width: 1.25rem;\n  height: 1.25rem;\n  background: #e3e4f1;\n  padding: 1px;\n  border-radius: 50%;\n\n  &--active,\n  &--completed {\n    background: linear-gradient(135deg, #55ddff 0%, #c058f3 100%);\n  }\n}\n\n.list-item__select {\n  background: white;\n  width: 100%;\n  height: 100%;\n  border-radius: 50%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n  &--completed {\n    background: linear-gradient(135deg, #55ddff 0%, #c058f3 100%);\n  }\n}\n.list-item__text {\n  align-self: center;\n}\n\n.list-item__cross {\n  width: 3.25rem;\n  padding: 4px 0 0 0;\n}\n\n.list-item__icon-cross {\n  width: 0.75rem;\n}\n.list__footer {\n  display: flex;\n  justify-content: space-between;\n  background: white;\n  color: #9495a5;\n  border-top: 1px solid #e3e4f1;\n  font-size: 0.75rem;\n}\n.list__items-left {\n  padding: 1.25rem 0 0 1.25rem;\n}\n.list__items-clear {\n  padding: 1.25rem 1.25rem 1.25rem 0;\n}\n.filters {\n  display: flex;\n  justify-content: space-evenly;\n  background: white;\n  color: #9495a5;\n  font-size: 0.875rem;\n  font-weight: 700;\n  padding: 1rem 0;\n  margin: 1rem 0 0 0;\n  &__item {\n    font-weight: 700;\n  }\n}\n// utils\n\n.hidden {\n  display: none;\n}\n","src/sass/base/_base.scss":"* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nhtml {\n  font-size: 16px;\n}\n\nbody {\n  font-family: \"Josefin Sans\", \"sans-serif\";\n}\n\ninput {\n  font-family: inherit;\n  border-style: none;\n}\n\nli {\n  list-style: none;\n}\n\nb